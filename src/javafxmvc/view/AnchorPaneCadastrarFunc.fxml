<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="720.0" prefWidth="990.0" stylesheets="@../style.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="src.javafxmvc.controller.AnchorPaneCadastrarFuncController">
   <children>
      <Label layoutX="30.0" layoutY="30.0" styleClass="h1" stylesheets="@../style.css" text="Cadastro Funcionário" />
      <Group layoutX="70.0" layoutY="141.0">
         <children>
            <Label text="Nome Completo" />
            <TextField fx:id="textFieldNome" layoutY="30.0" styleClass="input" stylesheets="@../style.css" />
            <Label fx:id="labelErroNome" layoutX="1.0" layoutY="87.0" styleClass="error" stylesheets="@../style.css" />
         </children>
      </Group>
      <Group layoutX="540.0" layoutY="141.0">
         <children>
            <Label text="CPF" />
            <TextField fx:id="textFieldCPF" layoutX="-1.0" layoutY="25.0" styleClass="input" stylesheets="@../style.css" />
            <Label fx:id="labelErroCPF" layoutX="1.0" layoutY="87.0" styleClass="error" stylesheets="@../style.css" />
         </children>
      </Group>
      <Group layoutX="70.0" layoutY="252.0">
         <children>
            <Label text="Endereco" />
            <TextField fx:id="textFieldEndereco" layoutY="28.0" prefHeight="53.0" prefWidth="850.0" style="-fx-pref-width: 849;" styleClass="input" stylesheets="@../style.css" />
            <Label fx:id="labelErroEndereco" layoutX="1.0" layoutY="87.0" styleClass="error" stylesheets="@../style.css" />
         </children>
      </Group>
      <Group layoutX="70.0" layoutY="364.0">
         <children>
            <Label text="Usuario" />
            <TextField fx:id="textFieldUsuario" layoutY="28.0" styleClass="input" stylesheets="@../style.css" />
            <Label fx:id="labelErroUsuario" layoutX="1.0" layoutY="87.0" styleClass="error" stylesheets="@../style.css" />
         </children>
      </Group>
      <Group layoutX="540.0" layoutY="364.0">
         <children>
            <Label text="Tipo" />
            <ComboBox fx:id="comboBoxTipos" layoutY="28.0" prefWidth="150.0" styleClass="input" />
            <Label fx:id="labelErroTipo" layoutX="1.0" layoutY="87.0" styleClass="error" stylesheets="@../style.css" />
         </children>
      </Group>
      <Group layoutX="71.0" layoutY="478.0">
         <children>
            <Label text="Senha" />
            <PasswordField fx:id="passwordFieldSenha" layoutY="25.0" styleClass="input" />
            <Label fx:id="labelErroSenha" layoutX="1.0" layoutY="87.0" styleClass="error" stylesheets="@../style.css" />
         </children>
      </Group>
      <Group layoutX="541.0" layoutY="478.0">
         <children>
            <Label text="Confirmação de Senha" />
            <PasswordField fx:id="passwordFieldSenhaConfirmada" layoutX="-5.0" layoutY="28.0" styleClass="input" />
            <Label fx:id="labelErroSenhaConfirmada" layoutX="1.0" layoutY="87.0" styleClass="error" stylesheets="@../style.css" />
         </children>
      </Group>
      <Button fx:id="buttonCadastrar" layoutX="740.0" layoutY="618.0" mnemonicParsing="false" onAction="#cadastrar" styleClass="register" text="Cadastrar" />
   </children>
</AnchorPane>
